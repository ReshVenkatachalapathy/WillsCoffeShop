<Window x:Class="WillsCoffeShop.employee_HomePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WillsCoffeShop"
        mc:Ignorable="d"
        Title="employee_HomePage" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">

            <Label Name="UserLoggedinName" Content="" HorizontalAlignment="Center" FontSize="18" FontWeight="Bold" Width="283"/>
            <Label Name="UsernameLabel" Content="" HorizontalAlignment="Center" FontSize="14" Margin="0,10,0,20"/>
            <Label Content="Below is your dashboard view" HorizontalAlignment="Center" FontSize="14" Margin="0,10,0,20"/>
            <Button Content="View Profile" Width="200" Margin="0,10,0,0" Click="ViewProfileButton_Click"/>
            <Button Content="View Payroll" Width="200" Margin="0,10,0,0" Click="ViewPayrollButton_Click"/>
        </StackPanel>
        <Button Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Margin="10,10,0,0" Click="BackButton_Click"/>
        <Button Content="Logout" HorizontalAlignment="Right" VerticalAlignment="Top" Width="80" Margin="0,10,10,0" Click="LogoutButton_Click"/>
    </Grid>
</Window>